int j=0;
		int i=1;
		int n=21;
		while(j<=n) {
			int sum=0;
			List<Integer> consecutiveSum=new ArrayList<>();
			for(int k=i;k<n;k++)
			{
				
				sum+=k;
				consecutiveSum.add(k);
				if(sum==n) {
					consecutiveSum.forEach(num->System.out.println(num));
					break;
				}
			}
			
			i++;
			j++;
		}
